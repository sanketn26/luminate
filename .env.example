# Luminate Environment Variables
# Copy this file to .env and update values

# JWT Authentication
JWT_SECRET=change-me-in-production

# ClickHouse Database
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=change-me-in-production

# Storage Backend (badger or clickhouse)
STORAGE_BACKEND=badger

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8080

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
